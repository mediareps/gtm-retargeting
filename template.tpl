___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// needed libraries
const query = require('queryPermission');
const setInWindow = require('setInWindow');
const injectScript = require('injectScript');

// needed vars
const tracking_name = data.tracking_name;
const adv_id = data.adv_id;
const expiry = data.expiry;
var THRetargetSettings = { tracking: tracking_name, adv_id: adv_id, expiry: expiry };
setInWindow('THRetargetSettings', THRetargetSettings, true);
const url = 'https://cdn1.traffichaus.com/scripts/retarget.js';

// inject the script
if (query('inject_script', url)) {
  injectScript(url, data.gtmOnSuccess, data.gtmOnFailure, url);
}


